package polymorphism;

public class TVUser {

	public static void main(String[] args) {
		System.out.println("Samsung TV");
		SamsungTV tv = new SamsungTV();
		tv.powerOn();
		tv.volumeUp();
		tv.volumeDown();
		tv.powerOff();

		System.out.println("Lg TV");
		LgTV tv = new LgTV();
		tv.powerOn();
		tv.volumeUp();
		tv.volumeDown();
		tv.powerOff();
	}
}
