package polymorphism;

public class BeanFactory {

	public Object getBean(String beanName) {
		System.out.println("Samsung TV");
		SamsungTV stv = new SamsungTV();
		stv.powerOn();
		stv.volumeUp();
		stv.volumeDown();
		stv.powerOff();

		System.out.println("\nLg TV");
		LgTV ltv = new LgTV();
		ltv.powerOn();
		ltv.volumeUp();
		ltv.volumeDown();
		ltv.powerOff();
	}
}
